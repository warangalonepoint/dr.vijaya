{
  "version": 2,
  "public": true,
  "cleanUrls": false,
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/dr.vijaya/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/dr.vijaya/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/dr.vijaya/$1"
    }
  ]
}
