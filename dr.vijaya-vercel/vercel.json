{
  "version": 2,
  "public": true,
  "cleanUrls": false,
  "rewrites": [
    { "source": "/", "destination": "/dr.vijaya-vercel/index.html" },
    { "source": "/(.*)", "destination": "/dr.vijaya-vercel/$1" }
  ],
  "headers": [
    { "source": "/service-worker.js", "headers": [{ "key": "Cache-Control", "value": "no-cache" }] },
    { "source": "/dr.vijaya-vercel/assets/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] }
  ]
}
